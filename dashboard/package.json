{
  "name": "ace-ux-components-client-side-solution",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "npm": "*",
    "yarn": "USE-NPM-INSTEAD",
    "node": "*"
  },
  "engineStrict": true,
  "main": "lib/index.js",
  "scripts": {
    "audit": "npm audit --registry=https://registry.npmjs.org/",
    "audit-fix": "npm audit fix --registry=https://registry.npmjs.org/",
    "build": "gulp bundle",
    "clean": "gulp clean",
    "nuke": "npm cache clean --force && cd ../ux-components && rimraf node_modules && rimraf lib && rimraf package-lock.json && cd ../dashboard && rimraf lib && rimraf dist && rimraf node_modules && rimraf package-lock.json",
    "nuke-n-serve": "npm run nuke && npm run init-ace && npm run serve",
    "compile-lint-rules": "cd customLintRules/ && tsc unnecessaryElseRule.ts && cd ../",
    "compile-resx-transform": "cd resx/ && tsc parseLocFileAsObject.ts resx-as-export.ts && cd ../",
    "generate-card": "yo @microsoft/sharepoint",
    "init-ace": "cd ../ux-components && npm i && npm run build && cd ../dashboard && npm i && npm run build",
    "link-ace-ux": "cd ../ux-components && npm link && cd ../dashboard && npm link ace-ux-components",
    "pull-ace-ux": "cd ../ux-components && npm run build && npm link && cd ../dashboard && npm link ace-ux-components && npm run build",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "serve": "gulp serve -l --nobrowser",
    "start": "npm run serve",
    "trust-dev-cert": "gulp trust-dev-cert",
    "patch-adaptivecards": "node patches/adaptivecards/patch.js",
    "refreshVSToken": "vsts-npm-auth -F -config .npmrc",
    "upgrade-lib": "cd ../ux-components && npm run build && cd ../dashboard && npm i && npm run build",
    "upgrade-lib-full": "cd ../ux-components && rm -rf lib && rm -rf node_modules && npm i && npm run build && cd ../dashboard && npm update ux-components",
    "upgrade-lib-full-windows": "cd ../ux-components && del lib /Q && del node_modules /Q && npm i && npm run build && cd ../dashboard && npm update ux-components",
    "preinstall": "npx force-resolutions",
    "postinstall": "npm run patch-adaptivecards && npm run link-ace-ux",
    "analyze-bundle": "gulp serve -l --analyze --nobrowser"
  },
  "dependencies": {
    "@microsoft/ace-ux-components": "^1.4.0",
    "@microsoft/sp-adaptive-card-extension-base": "1.17.0",
    "@microsoft/sp-core-library": "1.17.0",
    "@microsoft/sp-property-pane": "1.17.0",
    "dayjs": "^1.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@microsoft/generator-sharepoint": "1.17.0",
    "@microsoft/rush-stack-compiler-4.5": "0.4.0",
    "@microsoft/sp-build-web": "1.17.0",
    "@microsoft/sp-module-interfaces": "1.17.0",
    "@microsoft/sp-tslint-rules": "1.17.0",
    "@types/es6-promise": "^3.3.0",
    "@types/node": "^17.0.21",
    "@types/webpack-env": "1.13.1",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "adaptivecards": "^2.11.1",
    "ajv": "~5.2.2",
    "babel-jest": "^26.6.3",
    "cpy-cli": "^3.1.1",
    "eslint": "^8.33.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gulp": "~4.0.2",
    "json5": "^2.2.3",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "tslint-etc": "^1.13.10",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.9.10",
    "vsts-npm-auth": "^0.42.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "yo": "^4.3.0"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5"
  },
  "resolutions": {
    "glob-parent": "^6.0.2",
    "validator": "13.7.0",
    "node-fetch": "^2.6.7",
    "es5-ext": "0.10.50",
    "qs": "^6.11.0",
    "json-schema": "^0.4.0",
    "node-forge": "^1.3.1",
    "nth-check": "^2.1.1",
    "tar": "^6.1.11",
    "trim-newlines": "^4.0.2",
    "fresh": "^0.5.2",
    "terser": "5.14.2",
    "scss-tokenizer": "^0.4.2",
    "follow-redirects": "1.15.1",
    "gulp-flatten": "^0.4.0",
    "loader-utils": "^2.0.4",
    "json5": "^2.2.2",
    "uglify-js": "^3.13.10",
    "express": "^4.17.3",
    "request": "^2.88.2"
  },
  "volta": {
    "extends": "../package.json"
  }
}
